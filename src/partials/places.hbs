<section id="places" class="section is-marginless dark-bg">
    <div class="container">
        <h1 class="title is-size-1 written-with-leds has-tram-yellow-text">Places</h1>
        <h2 class="subtitle is-size-2 written-with-leds has-tram-yellow-text">Only to drink at</h2>
    </div>
</section>

<section class="section is-marginless dark-bg">
    <div class="container">
        <h2 class="subtitle is-size-2 written-with-leds has-tram-yellow-text">Prices</h2>
        <nav class="level prices-level">
            <div class="level-item has-text-centered">
                <div>
                    <img src="assets/img/beer.svg" alt="Picture">
                    <p class="is-size-3 written-with-leds has-white-text">Low</p>
                </div> 
            </div>
            <div class="level-item has-text-centered">
                <div>
                    <img src="assets/img/wine.svg" alt="Picture">
                    <p class="is-size-3 written-with-leds has-white-text">Medium</p>
                </div> 
            </div>
            <div class="level-item has-text-centered">
                <div>
                    <img src="assets/img/cocktail.svg" alt="Picture">
                    <p class="is-size-3 written-with-leds has-white-text">High</p>
                </div> 
            </div>
        </nav>
    </div>
</section>

<section class="section is-marginless dark-bg">
    <div id="publine" class="container">
        {{#each pubs}}
            {{> partials/publine-entry}}
        {{/each}}
    </div>
</section>